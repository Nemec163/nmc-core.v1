{
  "name": "nmc-ui.v1",
  "version": "1.0.0",
  "description": "NMC UI Components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
