    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NMC/nmc-core.v1.git",
    "directory": "packages/nmc-ui.v1"
  },
  "author": "NMC",
  "license": "MIT",
  "keywords": [
    "ui",
    "components",
